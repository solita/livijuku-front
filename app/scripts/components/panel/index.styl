.panel
  border: 0
  box-shadow: none
  .panel-heading
    border: 0
    box-shadow: none
    border-radius: 3px
    h4
      font-weight: 600
      i
        margin-right 0.5em
  .panel-heading+.panel-collapse > .panel-body
    border: 0

.panel-body
  h4
    font-weight: 400
    text-transform: uppercase
    padding-bottom: 0.5em
  .row
    margin-bottom: 1em
